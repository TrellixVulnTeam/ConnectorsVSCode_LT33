{
  "name": "connectors",
  "displayName": "Connectors",
  "description": "Power Platform Connectors in code",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.61.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:connectors.addConnector",
    "onCommand:connectors.input"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "connectors.addConnector",
        "title": "Add Connector"
      },
      {
        "command": "connectors.input",
        "title": "Get user input"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p -w . --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.4",
    "@types/mocha": "^9.0.0",
    "@types/node": "^14.17.22",
    "@types/uuid": "^8.3.1",
    "@types/vscode": "^1.61.0",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "@vscode/test-electron": "^1.6.2",
    "eslint": "^7.32.0",
    "glob": "^7.1.7",
    "mocha": "^9.1.1",
    "ts-loader": "^9.2.5",
    "typescript": "^4.4.3",
    "webpack": "^5.52.1",
    "webpack-cli": "^4.8.0"
  },
  "dependencies": {
    "@openapitools/openapi-generator-cli": "^2.4.12",
    "npm": "^8.0.0",
    "python-shell": "^3.0.1",
    "uuid": "^8.3.2"
  }
}
